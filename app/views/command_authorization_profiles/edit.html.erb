<%= error_messages_for :command_authorization_profile %>

<% form_for(:command_authorization_profile, :url => command_authorization_profile_url(@command_authorization_profile), :html => { :method => :put }) do |f| %>
<table>
  <tr>
    <td><b>Name:</b></td> 
    <td><%= f.text_field :name %></td>
  </tr>
  <tr><td>&nbsp</td></tr>
  <tr>
    <td><%= submit_tag "Update" %></td>
    <td><input type=button value="Cancel" onclick="window.location.href='<%= command_authorization_profile_url(@command_authorization_profile) %>'" /></td>
    <td colspan="2">&nbsp</td>
  </tr>
</table>
<% end %>
