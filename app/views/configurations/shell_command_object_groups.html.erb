<b style="font-size: 16px;">Shell Command Object Groups</b><br /><br />

<% if @shell_command_object_groups.length != 0 %>
    <% for shell_command_object_group in @shell_command_object_groups %>
        <br /><hr />
      <% if (@session_user.admin? || @configuration_roles[@configuration.id] == 'admin') %>
        <% if !shell_command_object_group.in_use? %>
        <%= link_to "delete", shell_command_object_group_url(shell_command_object_group), :style => "color: #7B2024;",  :confirm => "Delete '#{shell_command_object_group.name}'?", :method => :delete  %>
        &nbsp &nbsp &nbsp
        <% end %>
        <%= link_to "rename", edit_shell_command_object_group_url(shell_command_object_group)  %>
         &nbsp &nbsp &nbsp
        <%= link_to "modify", shell_command_object_group_url(shell_command_object_group)  %>
      <% end %>
      <%= scog_description(shell_command_object_group) %>
    <% end %>
    <hr />
<% else %>
<b>No Shell Command Object Groups have been created.</b>
<% end %>
<br />


