<%= error_messages_for :network_object_group %>

<% form_for(:network_object_group, :url => network_object_group_url(@network_object_group), :html => { :method => :put }) do |f| %>
<table>
  <tr>
    <td><b>Name:</b></td> 
    <td><%= f.text_field :name %></td>
  </tr>
  <tr><td>&nbsp</td></tr>
  <tr>
    <td><%= submit_tag "Update" %></td>
    <td><input type=button value="Cancel" onclick="window.location.href='<%= network_object_group_url(@network_object_group) %>'" /></td>
    <td colspan="2">&nbsp</td>
  </tr>
</table>
<% end %>
